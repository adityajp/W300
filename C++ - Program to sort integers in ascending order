// Program to sort integers in ascending order.
#include<iostream>
using namespace std;
int main()
{
  int a[7],i,j,t;
	cout<<"Enter the elements of the integer array: "<<endl;
	for(i=0;i<7;i++)
	cin>>a[i];
	
	for(i=0;i<7;i++)
	{
		for(j=i+1;j<7;j++)
		if(a[i]>a[j])
		{
			t=a[i];
			a[i]=a[j];
			a[j]=t;
		}
	}
	cout<<"\n\n";
	cout<<"The sorted integer array is: "<<endl;
	for(i=0;i<7;i++)
	cout<<a[i]<<endl;
	

}
